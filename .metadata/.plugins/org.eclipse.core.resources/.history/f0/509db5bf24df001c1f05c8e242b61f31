package com.masai;

public class Parent {

	final int number;
	void method1() {
		System.out.println("method1 belong to parent class");
		System.out.println("Number: "+number);
//		number=20;
	}
	
	void method2() {
		System.out.println("method2 belong to parent class");
	}
	
	void method3() {
		System.out.println("method belong to parent class");
	}
	
	Parent(){
		this.number=10;
	}
}
