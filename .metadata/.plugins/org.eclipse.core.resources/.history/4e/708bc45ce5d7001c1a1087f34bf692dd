package com.masai;

public class Employee {
	
	String id="1234";
	String name="manoj";
	double salary=50000.00;
	String address="mahendargarh";
	
	void employeeDetail() {
		System.out.println("EmployeeDetail");
		System.out.println("id"+"-"+id);
		System.out.println("name"+"-"+name);
		System.out.println("salary"+"-"+address);
	}
}
