package com.bankDetail;

public class Account {

	private int accountid;
	private String accountType;
	private int balance;
	
	public boolean withdraw(int wid) {
		if(wid>0) {
			balance-=wid;
			System.out.println("Balance amount after withdraw: "+balance);
			return true;
		}
		else {
			return false;
		}
	}
}
