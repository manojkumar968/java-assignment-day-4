package com.ticketPriceCalculaton;

public class Main {

	public static void main(String[] args) {
		
		Ticket t1=new Ticket();
		t1.setAvailableTickets(25);
		t1.setPrice(100);
		t1.setTicketid(123);
		int totalPrice=t1.calculateTicketCost(2);
		
		System.out.println(t1.getAvailableTickets());
		System.out.println(t1.getPrice());
		System.out.println(t1.getTicketid());
		System.out.println(totalPrice);
	}
}
